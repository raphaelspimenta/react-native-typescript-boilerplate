{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["module-resolver", {
      "extensions": [".js", ".jsx", ".ts", ".tsx"],
      "alias": {
        "containers": "./src/containers",
        "components": "./src/components",
        "core": "./src/core",
        "navigation": "./src/navigation",
        "hooks": "./src/hooks"
      }
    }]
  ]
}
